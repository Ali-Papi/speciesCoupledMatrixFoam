FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default                             Euler;
}

gradSchemes
{
    default                             Gauss linear;
}

divSchemes
{
    default                             Gauss linear;
    div(phip,alpha)                     Gauss upwind;
    div(phig,alpha)                     Gauss limitedLinear 1;
}

laplacianSchemes
{
    default                             Gauss linear uncorrected;
}

interpolationSchemes
{
    default                             linear;
}

snGradSchemes
{
    default                             uncorrected;
}

