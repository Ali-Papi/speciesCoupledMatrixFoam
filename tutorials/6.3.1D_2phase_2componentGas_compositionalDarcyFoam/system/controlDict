FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         coupledMatrixFoam;

startFrom           startTime;

startTime           0;

stopAt              endTime;

endTime             10; //20000;

deltaT              1;

writeControl        adjustableRunTime;

writeInterval       1; //5000;

purgeWrite          0;

writeFormat         ascii;

writePrecision      6;

writeCompression    off;

timeFormat          general;

timePrecision       6;

runTimeModifiable   yes;

adjustTimeStep      yes;

maxCo               1;

maxDeltaT           10;

functions
{}


